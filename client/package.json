{
  "name": "gobra-ide",
  "displayName": "Gobra",
  "version": "0.0.1",
  "publisher": "viper",
  "description": "This extension provides interactive IDE features for verifying programs in Gobra.",
  "author": {
    "name": "Chair of Programming Methodology, ETH Zurich"
  },
  "license": "MPL-2.0 License",
  "repository": {
    "type": "git",
    "url": "https://gitlab.inf.ethz.ch/OU-PMUELLER/student-projects/gobra-ide"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "main": "./out/extension",
  "dependencies": {
    "vscode-languageclient": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "tslint": "^5.16.0",
    "typescript": "^3.6.4",
    "vscode": "^1.1.35",
    "vscode-uri": "2.1.1"
  },
  "scripts": {
    "vscode:prepublish": "npm run update-vscode && npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "postinstall": "vscode-install",
    "update-vscode": "vscode-install"
  },
  "activationEvents": [
    "onLanguage:gobra"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gobra",
        "extensions": [
          ".gobra"
        ],
        "aliases": [
          "Gobra"
        ]
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Gobra configuration",
      "properties": {
        "languageServerExample.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    }
  }
}
